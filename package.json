    "react-scripts": "5.0.1",
    "tailwindcss": "3.0.24"
  },
  "scripts": {
    "start": "next start",
    "prebuild": "echo 'Building Sanity to public/studio' && cd sanity && sanity build ../public/studio -y && cd .. ",
